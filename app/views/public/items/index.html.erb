<div class="col-xs-3">
  <h2>ジャンル検索</h2>
  <% @genres.each do |genre| %>
  <div>
    <ul>
      <li><%= genre.genre_name %></li>
    </ul>
  </div>
  <% end %>
</div>

<div class="col-xs-12">
  <h2>商品一覧</h2>
   <% @items.each do |item| %>
  <div>
    <%= item.item_image_id %>
    <p>
      <%= link_to item_path(item) do %>
      <%= item.item_name %>
      <% end %>
      </p>
    <p><%= item.tax_excluded_price %></p>
  </div>
  <% end %>
</div>